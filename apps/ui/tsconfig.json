{
  "compilerOptions": {
    "lib": [
      "ESNext",
      "DOM",
    ],
    "module": "CommonJS",
    "target": "ESNext",
    "moduleResolution": "node",
    "isolatedModules": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "types": [
      "vite/client"
    ],
    "noEmit": true,
    "baseUrl": ".",
  },
}
